{
  "name": "alldigitalrewards/carrier-tracking-sdk",
  "description": "Carrier Tracking SDK",
  "version": "1.0.0",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "AllDigitalRewards\\CarrierTracking\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AllDigitalRewards\\Tests\\": "./tests"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=phpcs.xml src",
    "fix-style": "phpcbf -p --standard=phpcs.xml src"
  }
}
